{
  "name": "book-store-api",
  "version": "1.0.0",
  "description": "Proyecto inicial para aprender las bases de GraphQL mediante la practica",
  "main": "build/app.js",
  "scripts": {
    "start": "node build/app.js",
    "build": "npx tsc -p . && ncp src/schema build/schema",
    "dev": "nodemon \"src/app.ts\" --exec \"ts-node\" \"src/app.ts\" -e ts,graphql,json",
    "lint": "npx eslint . --ext .ts",
    "lint.fix": "npx eslint . --ext .ts --fix"
  },
  "keywords": [
    "graphql",
    "graphql-basic",
    "unions",
    "query",
    "mutation",
    "book-store-apollo-server-express"
  ],
  "author": "Jessica Pozo <jessicapozo198715@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@graphql-tools/load-files": "^6.6.1",
    "apollo-server-express": "^3.11.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import-node": "^0.0.5",
    "graphql-tools": "^8.3.18",
    "ncp": "^2.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.3",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
